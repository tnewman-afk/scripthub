local PowerSpam = Window:NewTab("Power Spam")
local PowerSpamSection = PowerSpam:NewSection("Chaotic Scarlet Witch Powers")



PowerSpamSection:NewButton("Universal Kill-All", "This is on a loop", function(txt)
 print(txt)
 
 
       local Plr = game.Players.LocalPlayer
            for i, Player in pairs (game.Players:GetPlayers()) do

                if Player.Character ~= nil and Player ~= Plr then
                    local mag = (Player.Character.UpperTorso.Position - Plr.Character.UpperTorso.Position).magnitude
                    if mag < 100000 then
                        local Target = Player.Character or Player.CharacterAdded:Wait()
         
-- Script generated by Nut The Menace Spy - credits to Claudev0#8849

local args = {
    [1] = Target.RightLowerArm,
    [2] = Vector3.new(-2274.140625, 904.6631469726562, 235.2376251220703)
}

game:GetService("Players").LocalPlayer.Character.Skills.Desintegration.Fire:FireServer(unpack(args))


end
end
end

       local Plr = game.Players.LocalPlayer
            for i, Player in pairs (game.Players:GetPlayers()) do

                if Player.Character ~= nil and Player ~= Plr then
                    local mag = (Player.Character.UpperTorso.Position - Plr.Character.UpperTorso.Position).magnitude
                    if mag < 10000 then
                        local Target = Player.Character or Player.CharacterAdded:Wait()
         
-- Script generated by Nut The Menace Spy - credits to Claudev0#8849

local args = {
    [1] = Target.RightLowerArm,
    [2] = Vector3.new(-2274.140625, 904.6631469726562, 235.2376251220703)
}

game:GetService("Players").LocalPlayer.Character.Desintegration.Fire:FireServer(unpack(args))


end
end
end

       local Plr = game.Players.LocalPlayer
            for i, Player in pairs (game.Players:GetPlayers()) do

                if Player.Character ~= nil and Player ~= Plr then
                    local mag = (Player.Character.UpperTorso.Position - Plr.Character.UpperTorso.Position).magnitude
                    if mag < 1000 then
                        local Target = Player.Character or Player.CharacterAdded:Wait()
         

-- Script generated by Nut The Menace Spy - credits to Claudev0#8849

local args = {
    [1] = Target.RightLowerArm,
    [2] = Vector3.new(0,0,0)
}

game:GetService("Players").LocalPlayer.Character.Skills.Desintegration.Fire:FireServer(unpack(args))

end
end
end



       local Plr = game.Players.LocalPlayer
            for i, Player in pairs (game.Players:GetPlayers()) do

                if Player.Character ~= nil and Player ~= Plr then
                    local mag = (Player.Character.UpperTorso.Position - Plr.Character.UpperTorso.Position).magnitude
                    if mag < 100 then
                        local Target = Player.Character or Player.CharacterAdded:Wait()
         
-- Script generated by Nut The Menace Spy - credits to Claudev0#8849

local args = {
    [1] = Target.RightLowerArm,
    [2] = Vector3.new(-2274.140625, 904.6631469726562, 235.2376251220703)
}

game:GetService("Players").LocalPlayer.Character.Skills.Desintegration.Fire:FireServer(unpack(args))


end
end
end


end)

PowerSpamSection:NewLabel("Above works with all telekinesis users")

PowerSpamSection:NewButton("Mal's Turn everyone into goats", "This is on a loop", function(txt)
 print(txt)
 
 for i,v in pairs(game:GetService("Players"):GetChildren()) do 
 if v.Name ~= game.Players.LocalPlayer.Name   then  
     
-- Script generated by Nut The Menace Spy - credits to Claudev0#8849

local args = {
    [1] = v.Character.LeftLowerArm,
    [2] = Vector3.new(-2304.983154296875, 904.9352416992188, -673.33056640625)
}

game:GetService("Players").LocalPlayer.Character.Skills.Transfiguration.Fire:FireServer(unpack(args))

     
end
end

end)




